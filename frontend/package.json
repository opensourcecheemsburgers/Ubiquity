{
    "name": "ubiquity",
    "private": true,
    "version": "0.0.0",
    "description": "Ubiquity is a cross-platform music player written in Rust.",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "tailwind": "tailwindcss -i ./css/input.css -o ./css/output.css --watch",
        "tailwind:build": "tailwindcss -i ./css/input.css -o ./css/output.css  --minify",
        "start": "trunk serve",
        "start:backend": "cd src-tauri && cargo run tauri",
        "tauri": "tauri"
    },
    "devDependencies": {
        "@tailwindcss/typography": "^0.5.9",
        "daisyui": "^2.50.0",
        "tailwindcss": "^3.2.4"
    }
}
